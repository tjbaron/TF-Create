{
  "name": "TFCreate",
  "version": "0.0.1",
  "description": "Vector software.",
  "private": "true",
  "author": "Thomas Baron <thomasjbaron@hotmail.com>",
  "engine": "node 0.10.5",
  "main": "index.js",
  "bin": {
    "tfunite": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "node tests/index.js",
    "watch": "watchify client_scripts/index.js -o www/bundle.js"
  },
  "dependencies": {
    "async": "^1.4.0",
    "browserify-middleware": "^7.0.0",
    "connect": "^3.3.5",
    "connect-static": "^1.5.0",
    "filesaver.js": "^0.2.0",
    "gl-matrix": "^2.1.0",
    "jszip": "^2.5.0",
    "mathjs": "^3.17.0",
    "tfcommandline": "git+https://tfapps:tfapps888@bitbucket.org/twofront/tf-command-line.git",
    "tfcss": "0.0.4",
    "tfdom": "git+https://tfapps:tfapps888@bitbucket.org/twofront/tf-dom.git",
    "tfframework": "git+https://tfapps:tfapps888@bitbucket.org/twofront/tf-framework.git",
    "tflayout": "git+https://tfapps:tfapps888@bitbucket.org/twofront/tf-layout.git"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 5,
    "maxlen": 130,
    "globalstrict": true,
    "asi": false,
    "eqnull": false,
    "evil": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": true,
    "devel": true,
    "browser": true,
    "node": true
  }
}
